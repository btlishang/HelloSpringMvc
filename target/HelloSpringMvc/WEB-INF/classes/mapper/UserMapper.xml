<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lishang.web.dao">
    <select id="selectAll" resultType="UserInfo">
        SELECT
            a.username,
            a.password,
        FROM
            USER AS a
    </select>
</mapper>